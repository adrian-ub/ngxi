{
  "name": "ngxi",
  "type": "module",
  "version": "0.12.0-beta.2",
  "packageManager": "pnpm@10.6.2",
  "description": "Icon set for Angular applications",
  "author": "Adri√°n UB",
  "license": "MIT",
  "funding": "https://github.com/sponsors/adrian-ub",
  "homepage": "https://github.com/adrian-ub/ngxi#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/adrian-ub/ngxi.git"
  },
  "bugs": "https://github.com/adrian-ub/ngxi/issues",
  "keywords": [
    "icon",
    "iconify",
    "icons",
    "ngxi",
    "angular"
  ],
  "sideEffects": false,
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.18.0",
    "@angular/build": "^20.0.3",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@antfu/eslint-config": "^4.15.0",
    "@antfu/ni": "^25.0.0",
    "@iconify/json": "^2.2.351",
    "@iconify/utils": "^2.3.0",
    "@types/ejs": "^3.1.5",
    "@types/node": "^24.0.3",
    "bumpp": "^10.2.0",
    "ejs": "^3.1.10",
    "eslint": "^9.29.0",
    "lint-staged": "^16.1.2",
    "ora": "^8.2.0",
    "rxjs": "~7.8.0",
    "simple-git-hooks": "^2.13.0",
    "tinyexec": "^1.0.1",
    "tsx": "^4.20.3",
    "typescript": "~5.8.2",
    "vite": "^6.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
