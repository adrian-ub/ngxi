{
  "$schema": "http://json-schema.org/schema",
  "$id": "IconLibrary",
  "title": "Generate Icon Library",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the icon library",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What is the name of your icon library?"
    }
  },
  "required": [
    "name"
  ]
}
